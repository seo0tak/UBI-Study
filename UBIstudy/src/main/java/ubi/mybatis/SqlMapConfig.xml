<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN" 
  "http://mybatis.org/dtd/mybatis-3-config.dtd">
 
 <configuration>
 	<typeAliases>
 		<typeAlias type="ubi.model.UbiBean" alias="MyUbi"/>
 		<typeAlias type="ubi.admin.fboard.model.FboardBean" alias="AFboard"/>
 		<typeAlias type="ubi.admin.sboard.model.SboardBean" alias="ASboard"/>
 		<typeAlias type="ubi.admin.nboard.model.NboardBean" alias="ANboard"/>
 		<typeAlias type="ubi.admin.vboard.model.VboardBean" alias="AVboard"/>
 		<typeAlias type="ubi.admin.member.model.AdminMemberBean" alias="AMember"/>
 	</typeAliases>
 	<mappers>
 		<mapper resource="ubi/mybatis/ubi.xml"></mapper>
 		<mapper resource="ubi/mybatis/admin_fboard.xml"></mapper>
 		<mapper resource="ubi/mybatis/admin_sboard.xml"></mapper>
 		<mapper resource="ubi/mybatis/admin_nboard.xml"></mapper>
 		<mapper resource="ubi/mybatis/admin_vboard.xml"></mapper>
 		<mapper resource="ubi/mybatis/admin_member.xml"></mapper>
